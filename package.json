{
  "name": "qbitmc",
  "version": "3.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "start:ios": "ionic capacitor run ios -l --external",
    "start:android": "ionic capacitor run android -l --host=192.168.0.51",
    "build": "NODE_ENV=prod node secrets.mjs && ng build --localize",
    "build:test": "NODE_ENV=test node secrets.mjs && ng build --localize",
    "build:watch": "ng build --watch --localize --configuration development",
    "sync": "npm run sync:android & npm run sync:ios",
    "sync:android": "cap sync android",
    "sync:ios": "cap sync ios",
    "copy": "npm run copy:android & npm run copy:ios",
    "copy:android": "cap copy android",
    "copy:ios": "cap copy ios",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.4",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.4",
    "@angular/compiler": "^21.0.4",
    "@angular/core": "^21.0.4",
    "@angular/forms": "^21.0.4",
    "@angular/platform-browser": "^21.0.4",
    "@angular/platform-browser-dynamic": "^21.0.4",
    "@angular/router": "^21.0.4",
    "@ionic/angular": "^8.7.12",
    "@ngrx/effects": "^21.0.0-beta.0",
    "@ngrx/entity": "^21.0.0-beta.0",
    "@ngrx/eslint-plugin": "^21.0.0-beta.0",
    "@ngrx/operators": "^21.0.0-beta.0",
    "@ngrx/router-store": "^21.0.0-beta.0",
    "@ngrx/store": "^21.0.0-beta.0",
    "@ngrx/store-devtools": "^21.0.0-beta.0",
    "appwrite": "^21.5.0",
    "lottie-web": "^5.13.0",
    "ngx-lottie": "^20.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.0.3",
    "@angular/cli": "~21.0.3",
    "@angular/compiler-cli": "^21.0.4",
    "@angular/localize": "^21.0.4",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@ionic/angular-toolkit": "^12.3.0",
    "@ngrx/schematics": "^21.0.0-beta.0",
    "@types/jasmine": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@typescript-eslint/utils": "^8.33.1",
    "eslint": "8.57.x",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^9.0.11",
    "jasmine-core": "^5.13.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": "eslint --fix"
  }
}
